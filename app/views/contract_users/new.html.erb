<h2>New contract_user</h2>
<div id="footer-menu" class="box">
  <%= render_menu :menu_payment_fs %>
</div>

<% form_for(@contract_user) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :contract %><br />
    <%= f.text_field :contract %>
  </p>
  <p>
    <%= f.label :user %><br />
    <%= f.text_field :user %>
  </p>
  <p>
    <%= f.label :date %><br />
    <%= f.datetime_select :date %>
  </p>
  <p>
    <%= f.label :scadenza %><br />
    <%= f.datetime_select :scadenza %>
  </p>
  <p>
    <%= f.label :clicks %><br />
    <%= f.text_field :clicks %>
  </p>
  <p>
    <%= f.label :note %><br />
    <%= f.text_area :note %>
  </p>
  <p>
    <%= f.label :renewed %><br />
    <%= f.check_box :renewed %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', contract_users_path %>
