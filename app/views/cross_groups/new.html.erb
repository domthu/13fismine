<h2><%= link_to l(:label_cross_group_plural), cross_groups_path %> &#187; <%= l(:label_cross_group_new) %></h2>
<div id="footer-menu" class="box">
  <%= render_menu :menu_table_fs %>
</div>

<% labelled_form_for @cross_group do |f| %>

<%= render :partial => 'form', :locals => { :f => f } %>

<p>
  <%= f.submit l(:button_create) %>
  <%= f.submit l(:button_create_and_continue), :name => 'continue' %>
</p>
<% end %>
