<div class="fs-layout-cell fs-content clearfix" style="width: 99%; position:relative;">
  <div id="fs-edizione">
    <%= link_to(image_tag("/images/ico/newsletter.png"), url_for(:controller => 'editorial', :action => "edizione_newsletter", :escape => false), :height => 64) %>
    <%= @project.name %>
  </div>
  <div style="text-align: center;">
    <%= @project.description.nil? ?  '' : @project.description  %>
  </div>
  <br/>
  <div class="cleared" style="width: 100%; margin:11px;"></div>
  <%= render :partial => 'articoli', :locals => {:issues => @issues, :issues_pages => @issues_pages, :issues_count => @issues_count} %>
    </div>
<%= render :partial => 'blocco_sidebar1' %>


