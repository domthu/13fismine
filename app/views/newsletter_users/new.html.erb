<h1>New newsletter_user</h1>

<% form_for(@newsletter_user) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :email_type_id %><br />
    <%= f.text_field :email_type_id %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :convention_id %><br />
    <%= f.text_field :convention_id %>
  </p>
  <p>
    <%= f.label :html %><br />
    <%= f.text_area :html %>
  </p>
  <p>
    <%= f.label :newsletter_id %><br />
    <%= f.text_field :newsletter_id %>
  </p>
  <p>
    <%= f.label :errore %><br />
    <%= f.text_area :errore %>
  </p>
  <p>
    <%= f.label :sended %><br />
    <%= f.check_box :sended %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', newsletter_users_path %>
