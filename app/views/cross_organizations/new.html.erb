<h1>New cross_organization</h1>

<% form_for(@cross_organization) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :organizzazione %><br />
    <%= f.text_field :organizzazione %>
  </p>
  <p>
    <%= f.label :sigla %><br />
    <%= f.text_field :sigla %>
  </p>
  <p>
    <%= f.label :se_visibile %><br />
    <%= f.check_box :se_visibile %>
  </p>
  <p>
    <%= f.label :type_organization_id %><br />
    <%= f.text_field :type_organization_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', cross_organizations_path %>