<%# yield :header_tags -%>
</head>
<body>
<div id="fs-main">
  <header class="fs-header clearfix">
    <div class="fs-shapes">
      <div id="top-menu-fe">
        <div id="account-fe">
          <%= render_menu :account_fe_menu -%>
          <%#render_menu :account_fe_menu -%>
        </div>
        <%= content_tag(
                    'div',
                    "#{l(:label_logged_as)} #{link_to_user(User.current, :format => :username)}".html_safe,
                    :id => 'loggedas-fe') if User.current.logged? %>
        <%# render_menu :top_menu if User.current.logged? || !Setting.login_required? -%>
        <%=render_menu :application_menu if !User.current.logged? %>
      </div>

      <h1 class="fs-headline" >
        <a href="#">Professionisti Esperti in Materie Fiscali Sportive</a>
      </h1>

      <div class="fs-logo" ></div>


    <div class="fs-textblock fs-cerca">
      <form class="fs-search" name="Search" action="javascript:void(0)">
        <input type="text" value="">
        <input type="submit" value="Search" name="search" class="fs-search-button">
      </form>
    </div></div>
    <nav class="fs-nav clearfix">
      <%= render_navigation(:level => 1) %>
    </nav>
  </header>
  <div class="fs-sheet-nav clearfix">
    <nav class="fs-2nav clearfix">
      <%= render_navigation :level => 2 %>
    </nav>
  </div>
  <div class="fs-sheet clearfix">
    <div class="fs-layout-wrapper clearfix">
      <div class="fs-content-layout">
        <div class="fs-content-layout-row">
          <%= yield %>
        </div>
      </div>
    </div>
    <%#sidebar a destra %>
    <%# render :partial => 'footer' %>
  </div>

</div> <%#fs-main%>
<div id="ajax-indicator" style="display:none;"><span><%= l(:label_loading) %></span></div>
<div id="header">
  <div id="main-menu" class="fs_box_opacity">
    <%= render_menu :application_menu_fs %>
    <br/>
    <%= render_menu :footer_menu_fs %>
  </div>
</div>

</body>
</html>
