  <%= javascript_include_tag "jquery.carouFredSel-6.1.0.js" %>
  <script type="text/javascript">
      //$(function () {
      $(document).ready(function() {
          'use strict';
          if ($.fn.slider) {
              $(".fs-slidecontainerslideshutterstock_1").each(function () {
                  var slideContainer = $(this), tmp;
                  var inner = $(".fs-slider-inner", slideContainer);
                  var helper = null;
                  inner.children().eq(0).addClass("active");
                  slideContainer.slider({
                      pause:2600,
                      speed:600,
                      repeat:true,
                      animation:"fade",
                      direction:"next",
                      navigator:slideContainer.siblings(".fs-slidenavigatorslideshutterstock_1"),
                      helper:helper
                  });
              });
          };
          $("#carousel_assos_js").carouFredSel({
              items	 : 3,
              scroll : {
                  items			    : 3,
                  easing			  : "elastic",
                  duration		  : 2000,
                  pauseOnHover	: true
              }
          });
          $("#carousel_tramenu_js").carouFredSel({
          	responsive	: true,
          	scroll		: {
          		fx			: "crossfade"
          	},
          	items		: {
          		visible		: 1,
          		width		: 290,
          		height		: 94
          	}
          });
          //$("#assos_carousel").removeClass("hide");
          // $("input[type=submit]").button();

          //Focus
          //< %= javascript_tag "$('#username').focus();" %>
          $('#username').focus();
          //Scroll to top
          $(window).scroll(function() {
              if(jQuery(this).scrollTop() != 0) {
                  jQuery('#toTop').fadeIn();
              } else {
                  jQuery('#toTop').fadeOut();
              }
          });

          $('#toTop').click(function() {
              jQuery('body,html').animate({scrollTop:0},800);
          });

          $('.x-white').addGlow({ textColor: 'white', haloColor: '#aaa', radius: 100 });
          $('.x-blue').addGlow({ textColor: '#00f', haloColor: '#00f', radius: 100 });
          $('.x-yellow').addGlow({ textColor: '#F8DA00', haloColor: '#F8DA00', radius: 100 });
          $('.x-red').addGlow({ textColor: '#f00', haloColor: '#f00', radius: 100 });
          $('.x-granata').addGlow({ textColor: '#a03823', haloColor: '#a03823', radius: 100 });

      });

  </script>
  <!-- page specific tags -->
  <%= yield :header_tags -%>
    <!-- end page specific tags -->

</head>
<body>
<div id="fs-main">
  <div id="fs-header-bg" class="clearfix">
  </div>
  <div class="fs-sheet clearfix">
    <header class="fs-header clearfix">


      <div class="fs-shapes">
        <h1 class="fs-headline" data-left="58%">
          <a href="#">Professionisti esperti in materie fiscali sportive</a>
        </h1>


      </div>
      <div class="fs-textblock fs-object375974515" data-left="80%">
        <% form_tag({:controller => 'editorial', :action => 'ricerca'}, :method => :get, :class => "fs-search") do %>
            <%= text_field_tag 'q', @question, :accesskey => accesskey(:quick_search) %>
            <input type="submit" value="ricerca" class="fs-search-button"/>
        <% end %>
      </div>

    </header>

    <nav class="fs-nav clearfix">
      <%= render_navigation(:level => 1) %>
    </nav>
    <div id="fs-tramenu-wrapper">
      <%= render :partial => 'editorial/tramenu_block' %>

    </div>
    <nav class="fs-menu2-nav clearfix">
      <%= render_navigation(:level => 2..3, :expand_all => true) %>
    </nav>

    <div id="user-info">
    <%= render_flash_messages %>
    </div>

    <div class="fs-layout-wrapper clearfix">
      <div class="fs-content-layout">
        <div class="fs-content-layout-row">
          <%= yield %>
        </div>
      </div>
    </div>
    <div id="toTop" style="display: none;"></div>
    <div id="ajax-indicator" style="display: none;"><span><%= l(:label_loading) %></span></div>
    <footer class="fs-footer clearfix">
      <p><a href="#">Link1</a> | <a href="#">Link2</a> | <a href="#">Link3</a></p>

      <p>Copyright Â© 2012. All Rights Reserved.</p>

    </footer>

  </div>
  <div class="fs-page-footer">
    <div class="clearfix"></div>
    <span id="fs-footnote-links"><a href="http://www.fiscosport.it/" target="_blank">Privacy </a> | site created by collaborations.</span>
  </div>
</div>

</body>
</html>
