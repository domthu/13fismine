<%# heads_for_theme %>
  <!--[if IE 6]><!--
    <style type="text/css">
      * html body{ width: expression( document.documentElement.clientWidth < 900 ? '900px' : '100%' ); }
      body {behavior: url(<%= stylesheet_path "csshover.htc" %>);}
    </style>
<![endif]-->
<%#call_hook :view_layouts_editorial_html_head %>
  <!-- page specific tags -->
<%# yield :header_tags -%>
</head>
<body>
<div id="fs-main">
  <header class="fs-header clearfix">


    <div class="fs-shapes">
      <h1 class="fs-headline" data-left="2.62%">
        <a href="#">Professionisti Esperti in Materie Fiscali Sportive</a>
      </h1>

      <div class="fs-logo" data-left="100%"></div>

    </div>
    <div class="fs-textblock fs-cerca" data-left="3.07%">
      <form class="fs-search" name="Search" action="javascript:void(0)">
        <input type="text" value="">
        <input type="submit" value="Search" name="search" class="fs-search-button">
      </form>
    </div>
    <nav class="fs-nav clearfix">
      <%= render_navigation(:level => 1) %>
    </nav>
    <nav class="fs-2nav clearfix">
      <%= render_navigation  :level => 2  %>
    </nav>
  </header>
  <div class="fs-sheet clearfix">
    <div class="fs-layout-wrapper clearfix">
      <div class="fs-content-layout">
        <div class="fs-content-layout-row">
          <div class="fs-layout-cell fs-content clearfix">
            <%= yield %>
          </div>
          <%#sidebar a centro destra %>
          <%= render :partial => 'blocco_sidebar1' %>
          <%#sidebar a destra %>
          <%= render :partial => 'blocco_sidebar2' %>
        </div>
      </div>
    </div>
              <%#sidebar a destra %>
          <%= render :partial => 'footer' %>
  </div>

</div> <%#fs-main%>
<div id="ajax-indicator" style="display:none;"><span><%= l(:label_loading) %></span></div>
<div id="header">
  <div id="main-menu" class="fs_box_opacity">
    <%= render_menu :application_menu_fs %>
    <br/>
    <%= render_menu :footer_menu_fs %>
  </div>
</div>

</body>
</html>
