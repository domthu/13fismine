<h2><%= link_to l(:label_type_organization_plural), type_organizations_path %> &#187; <%= l(:label_type_organization_new) %></h2>

<% labelled_form_for @type_organization do |f| %>
<%= render :partial => 'form', :locals => { :f => f } %>
<p>
  <%= f.submit l(:button_create) %>
  <%= f.submit l(:button_create_and_continue), :name => 'continue' %>
</p>
<% end %>
